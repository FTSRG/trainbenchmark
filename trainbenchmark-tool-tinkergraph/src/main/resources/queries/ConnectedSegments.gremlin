g.V()
    .filter { it.get().label() == 'Segment' }.as('segment1')
    .out('sensor').as('sensor')
    .back('segment1').out('connectsTo').as('segment2')
    .out('connectsTo').as('segment3')
    .out('connectsTo').as('segment4')
    .out('connectsTo').as('segment5')
    .out('connectsTo').as('segment6')
    .out('sensor').retain('sensor')
    .select('sensor', 'segment1', 'segment2', 'segment3', 'segment4', 'segment5', 'segment6')
