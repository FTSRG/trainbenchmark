PREFIX  base: <http://www.semanticweb.org/ontologies/2015/trainbenchmark#>
PREFIX  rdf:  <http://www.w3.org/1999/02/22-rdf-syntax-ns#>

SELECT  (COUNT(DISTINCT ?schedule2) AS ?count)
WHERE
  { ?schedule1 rdf:type base:Station .
    ?schedule2 rdf:type base:Station .
    ?schedule1 base:neighbors ?schedule2 .
    ?schedule2 base:neighbors ?schedule1
  }
