PREFIX  base: <http://www.semanticweb.org/ontologies/2015/trainbenchmark#>
PREFIX  rdf:  <http://www.w3.org/1999/02/22-rdf-syntax-ns#>

SELECT  ?x (( COUNT(?o) + COUNT(?s) ) AS ?degree)
WHERE
  { { ?x rdf:type base:Station}
      { ?x base:neighbors ?o
        FILTER ( ( ! isLiteral(?o) ) )
      }
    UNION
      { { ?x rdf:type base:Station}
        { ?s base:neighbors ?x
        }
      }
  }
GROUP BY ?x
