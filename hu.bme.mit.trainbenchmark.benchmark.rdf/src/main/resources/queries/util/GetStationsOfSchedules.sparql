PREFIX  base: <http://www.semanticweb.org/ontologies/2015/trainbenchmark#>
PREFIX  rdf:  <http://www.w3.org/1999/02/22-rdf-syntax-ns#>

SELECT  ?schedule ?station
WHERE
  { 
    { ?schedule rdf:type base:Schedule}
    { ?station rdf:type base:Station}
    { ?schedule ?p ?station}
  }
ORDER BY ?schedule
