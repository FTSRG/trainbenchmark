PREFIX  base: <http://www.semanticweb.org/ontologies/2015/trainbenchmark#>
PREFIX  rdf:  <http://www.w3.org/1999/02/22-rdf-syntax-ns#>

SELECT  ?attribute (COUNT(?association) AS ?count)
WHERE
  { ?association rdf:type base:Association .
    ?association base:category ?attribute
  }
GROUP BY ?attribute
