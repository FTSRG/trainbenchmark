package hu.bme.mit.trainbenchmark.railway

query "SwitchSet check"
    route : Route( )
    Signal( this == route.route_entry, signal_currentState == SignalState.GO )
    switchPosition : SwitchPosition( this memberOf route.route_switchPosition, $switchPosition_switch : switchPosition_switch )
    Switch( this == switchPosition.switchPosition_switch, switch_currentState.value != switchPosition.switchPosition_switchState.value )
end
