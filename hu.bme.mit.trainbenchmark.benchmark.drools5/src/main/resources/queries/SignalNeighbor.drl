    package Concept

    query "SignalNeighbor check"
        route : Route( )
        sen1A : Sensor( ) from route.route_routeDefinition
        te1A : Trackelement( ) from sen1A.sensor_trackElement
        te2A : Trackelement( ) from te1A.trackElement_connectsTo
        sen2A : Sensor( ) from te2A.trackElement_sensor
        sigA : Signal( this == route.route_exit )
        Route( route_routeDefinition contains sen2A, this != route )
        not Route( route_entry == sigA, route_routeDefinition contains sen2A )
    end
